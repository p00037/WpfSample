<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:CustomContorol="clr-namespace:WpfBase.CustomContorol"
    xmlns:local="clr-namespace:WpfBase">
    <Style TargetType="{x:Type CustomContorol:WindowBase}">
        <Setter Property="Background" Value="#FFEEF4FB" />
    </Style>

    <Style TargetType="{x:Type CustomContorol:CustomControl1}">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type CustomContorol:CustomControl1}">
                    <Grid Background="#FFEEF4FB">
                        
                    </Grid>
                    <!--<Border Background="{TemplateBinding Background}"
                            BorderBrush="{TemplateBinding BorderBrush}"
                            BorderThickness="{TemplateBinding BorderThickness}">
                    </Border>-->
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
</ResourceDictionary>
